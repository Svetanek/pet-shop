  "source": "pragma solidity ^0.5.0;\n\n\ncontract Adoption {\n  address[4] public adopters;\n\n  function getAdopters() public view returns(address[4] memory) {\n    return adopters;\n  }\n\n  function adopt(uint petId) public returns(uint) {\n    require(petId >= 0 && petId <= 3);\n    adopters[petId] = msg.sender;\n    return petId;\n  }\n}\n",
